function preload() {
 //font= loadFont('data/RobotoMedium.ttf');
 font= loadFont('data/Syncopate-Bold.ttf');
}

function setup() {
  createCanvas(1100,1100);

  textSize(32);
  textFont(font);
  textAlign(CENTER);
}
function draw() {
  noStroke();
  fill(21,123,222);
  rect (0,0,1100,1100);
  fill (234,123,111);
  textSize(60)
  text ('Raymond',600,300);
  textSize (20);
  text ('Small',600,150);
  textSize (15)
  text ('smaller',600,500);
}
